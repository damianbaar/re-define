'<%= f.name %>': [function(require, module, exports) { <%= f.contents %>
}]<% if(arr.length - 1 > i) { %>, <% }}) %>
}
, function() { <%= util.namespace(config.namespace, 'this') %> return <%= config.namespace %> }.call(this)
, [<%= config.imports %>]
